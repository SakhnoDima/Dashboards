{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.html",
      "continue": true
    }
  ],
  "env": {
    "VITE_SOME_KEY_ID": "${VITE_SOME_KEY_ID}",
    "VITE_SOME_ACCESS_KEY": "${VITE_SOME_ACCESS_KEY}",
    "VITE_SOME_MIXPANEL_TOKEN": "${VITE_SOME_MIXPANEL_TOKEN}"
  }
}
